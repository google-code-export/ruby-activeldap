
<div class="object-classes">
  <table class="values">
    <thead>
      <tr>
        <th><%= _("objectClass name") %></th>
        <th><%= _("description") %></th>
      </tr>
    </thead>

    <tbody>
<% object_classes.uniq.sort.each do |object_class| -%>
      <tr class="<%= cycle('even', 'odd') %>">
        <td><%=h loc_(object_class) %></td>
        <td><%=h locd_(object_class) %></td>
      </tr>
<% end -%>
    </tbody>
  </table>
</div>
<% reset_cycle %>
